# API Keys for Universal AI x Supply Chain Management Modules
# Copy this file to .env and add your actual API keys

# OpenAI API Key (required for LLM_API_Basics, Agent_SCM_Game, and some RAG modules)
OPENAI_API_KEY=your_openai_api_key_here

# DeepSeek API Key (alternative LLM provider for cost-effective inference)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Hugging Face API Key (for accessing Hugging Face models and datasets)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Optional: Set to True to enable debug logging
DEBUG=False

# Optional: Set default model preferences
DEFAULT_LLM_MODEL=gpt-4
DEFAULT_EMBEDDING_MODEL=all-MiniLM-L6-v2
